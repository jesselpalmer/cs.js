{
  "name": "cs.js",
  "version": "0.0.0",
  "description": "CS algorithms and data structures in JavaScript",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d lib",
    "build:watch": "babel src -w --out-dir lib",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "lint": "standard src",
    "lint:fix": "standard --fix",
    "test": "npm run lint && npm run cover"
  },
  "repository": "git+https://github.com/jesselpalmer/cs.js.git",
  "keywords": [
    "CS",
    "Computer Science",
    "Algorithms",
    "Data Structures"
  ],
  "author": "Jesse Palmer <jesselpalmer@gmail.com> (http://jesselpalmer.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/cs.js/issues"
  },
  "homepage": "https://github.com/jesselpalmer/cs.js#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-env": "1.5.2",
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "standard": "^10.0.2"
  }
}
