{
  "name": "cs.js",
  "version": "0.0.2",
  "description": "CS algorithms and data structures in JavaScript",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "build:watch": "babel src -w --out-dir lib",
    "cover": "./node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -R spec lib/**/*.spec.js",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint:fix": "./node_modules/.bin/eslint --fix ./src",
    "test": "npm run lint && npm run cover",
    "test:unit": "./node_modules/.bin/mocha './src/**/*.spec.js' --require @babel/register",
    "test:unit:watch": "./node_modules/.bin/mocha './src/**/*.spec.js' -w --require @babel/register",
    "test:unit:lib": "./node_modules/.bin/mocha './lib.spec.js' --require @babel/register",
    "test:unit:lib:watch": "./node_modules/.bin/mocha './lib.spec.js' -w --require @babel/register"
  },
  "repository": "https://github.com/jesselpalmer/cs.js",
  "engines": {
    "node": ">=4.2.4"
  },
  "keywords": [
    "Algorithms",
    "CS",
    "CS Algoorithms",
    "Computer Science",
    "Data Structures"
  ],
  "author": "Jesse Palmer <jesselpalmer@gmail.com> (http://jesselpalmer.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/cs.js/issues"
  },
  "homepage": "https://github.com/jesselpalmer/cs.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.2",
    "mocha": "^7.1.1"
  },
  "private": false
}
