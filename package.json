{
  "name": "cs.js",
  "version": "0.0.2",
  "description": "CS algorithms and data structures in JavaScript",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "build:watch": "babel src -w --out-dir lib",
    "cover": "./node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -R spec lib/**/*.spec.js",
    "lint": "./node_modules/.bin/standard",
    "lint:fix": "./node_modules/.bin/standard --fix",
    "test": "npm run lint && npm run cover",
    "test:library": "NODE_ENV=test mocha 'lib.spec.js' --compilers js:babel-register",
    "test:unit": "NODE_ENV=test mocha './src/**/*.spec.js' --compilers js:babel-register",
    "test:unit:watch": "NODE_ENV=test mocha './src/**/*.spec.js' -w --compilers js:babel-register",
    "test:unit:watch:lib": "NODE_ENV=test mocha './lib.spec.js' -w --compilers js:babel-register"
  },
  "repository": "https://github.com/jesselpalmer/cs.js",
  "engines": {
    "node": ">=4.2.4"
  },
  "keywords": [
    "Algorithms",
    "CS",
    "CS Algoorithms",
    "Computer Science",
    "Data Structures"
  ],
  "author": "Jesse Palmer <jesselpalmer@gmail.com> (http://jesselpalmer.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/cs.js/issues"
  },
  "homepage": "https://github.com/jesselpalmer/cs.js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.24.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "standard": "^12.0.1"
  },
  "standard": {
    "ignore": [
      "/lib",
      "lib.spec.js"
    ]
  },
  "private": false
}
